{
  "name": "delp.pt",
  "version": "1.0.37",
  "description": "Meta Simple Portuguese Dictionary",
  "main": "index.js",
  "scripts": {
    "test": "standard && node test.js",
    "postinstall": "node server.js",
    "updatePackages": "ncu -e 2 || (ncu -u && npm i --ignore-scripts && npm run commitAndUpload)",
    "commitAndUpload": "npm test && git commit -a -m 'update' && npm version patch && npm publish && git push",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfoclpf/delp.pt"
  },
  "author": "jfolpf",
  "license": "GPL-3.0",
  "homepage": "https://delp.pt",
  "keywords": [
    "dictionary",
    "portuguese"
  ],
  "dependencies": {
    "async": "3.2.0",
    "body-parser": "1.19.0",
    "colors": "1.4.0",
    "command-line-args": "5.1.1",
    "compression": "1.7.4",
    "debug": "4.3.0",
    "express": "4.17.1",
    "express-handlebars": "4.0.5",
    "html2json": "1.0.2",
    "jquery": "3.5.1",
    "jsdom": "16.3.0",
    "mysql": "2.18.1"
  },
  "devDependencies": {
    "html-minifier": "4.0.0",
    "pretty": "2.0.0",
    "snyk": "^1.424.4",
    "standard": "14.3.3",
    "words-pt": "1.0.19"
  },
  "snyk": true
}
